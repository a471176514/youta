<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youta.account.mapper.AccountMapper">
    
    <select id="queryAccount" resultType="com.youta.account.model.Account" parameterType="long">
        select id,name
        from account
        where id = #{id}
    </select>

</mapper>